{
  "project": {
    "name": "czarina-v0.7.0",
    "slug": "czarina-v0_7_0",
    "version": "0.7.0",
    "phase": 1,
    "description": "Memory System + Agent Rules Integration",
    "repository": "/home/jhenry/Source/czarina",
    "orchestration_dir": ".czarina"
  },
  "orchestration": {
    "mode": "local",
    "auto_push_branches": false
  },
  "omnibus_branch": "cz1/release/v0.7.0",
  "workers": [
    {
      "id": "rules-integration",
      "role": "code",
      "agent": "claude",
      "branch": "cz1/feat/rules-integration",
      "description": "Integrate agent rules library into Czarina",
      "dependencies": []
    },
    {
      "id": "memory-core",
      "role": "code",
      "agent": "claude",
      "branch": "cz1/feat/memory-core",
      "description": "Implement memory file structure and basic I/O",
      "dependencies": []
    },
    {
      "id": "memory-search",
      "role": "code",
      "agent": "claude",
      "branch": "cz1/feat/memory-search",
      "description": "Build semantic search with vector embeddings",
      "dependencies": []
    },
    {
      "id": "cli-commands",
      "role": "code",
      "agent": "aider",
      "branch": "cz1/feat/cli-commands",
      "description": "Add czarina memory CLI commands",
      "dependencies": []
    },
    {
      "id": "config-schema",
      "role": "code",
      "agent": "claude",
      "branch": "cz1/feat/config-schema",
      "description": "Extend config.json schema for rules and memory",
      "dependencies": ["rules-integration", "memory-core"]
    },
    {
      "id": "launcher-enhancement",
      "role": "code",
      "agent": "claude",
      "branch": "cz1/feat/launcher-enhancement",
      "description": "Enhance agent launcher to load rules and memory",
      "dependencies": ["config-schema", "memory-search", "rules-integration"]
    },
    {
      "id": "integration",
      "role": "qa",
      "agent": "claude",
      "branch": "cz1/feat/integration",
      "description": "Merge all branches and perform E2E testing",
      "dependencies": ["rules-integration", "memory-core", "memory-search", "cli-commands", "config-schema", "launcher-enhancement"],
      "merges": ["rules-integration", "memory-core", "memory-search", "cli-commands", "config-schema", "launcher-enhancement"]
    },
    {
      "id": "documentation",
      "role": "documentation",
      "agent": "claude",
      "branch": "cz1/feat/documentation",
      "description": "Create complete documentation and migration guide",
      "dependencies": ["integration"]
    },
    {
      "id": "release",
      "role": "qa",
      "agent": "claude",
      "branch": "cz1/feat/release",
      "description": "Final QA, tag v0.7.0, and publish release",
      "dependencies": ["documentation", "integration"],
      "merges": ["integration", "documentation"]
    }
  ],
  "daemon": {
    "enabled": true,
    "auto_approve": ["read", "write", "commit"]
  },
  "memory": {
    "enabled": false,
    "note": "Will be implemented by this orchestration"
  },
  "agent_rules": {
    "enabled": false,
    "note": "Will be implemented by this orchestration"
  }
}
